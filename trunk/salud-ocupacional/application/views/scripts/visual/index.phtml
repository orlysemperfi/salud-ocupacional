<script type="text/javascript">
    $(document).ready(function(){
        refrescar();
    });
    function refrescar(){
        $.ajax({
            url: '/visual/refrescarpersonal/',
            success: function(data) {
                $("#divVcontenidozq").empty();
                $.each($.parseJSON(data), function(i,row){
                    $("#divVcontenidozq").append('<div class="divVitempersonal">\n\
                            <div class="divVitempoint"><img src="/images/disponible.gif" /></div>\n\
                            <div class="divVitemnombre">'+row.nombre+'</div>\n\
                            <div class="divVitemtitulo">'+row.titulo+'</div>\n\
                            </div>');
                });
            }
        });
        $.ajax({
            url: '/visual/refrescarpacientes/',
            success: function(data) {
                $("#divVcontenidoder").empty();
                $.each($.parseJSON(data), function(i,row){
                    $("#divVcontenidoder").append('<div class="divVitempaciente">\n\
                                <div class="divVitema">'+row.dispon+'</div>\n\
                                <div class="divVitemb">'+row.nombre+'</div>\n\
                                <div class="divVitemc">'+row.motivo+'</div>\n\
                                <div class="divVitemd">'+row.examen+'</div></div>');
                });
            }
        });
        setTimeout("refrescar()", 5000);
    }
</script>
