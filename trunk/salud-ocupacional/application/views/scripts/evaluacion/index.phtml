                if(obj == 3){
                    $('#ficAne7C', menu).remove();
                }
                if(obj != 7){
                    $('#pasVis', menu).remove();
                }
                if(obj != 2){
                    $('#evaRes', menu).remove();
                }
                if(obj == 6 || obj == 7 || obj == 2){
                    $('#evaMedFisPs', menu).remove();
                }
                return menu;
            }
        }
        $("#btnActualizar").button({ icons: { primary: "ui-icon-refresh" } });
        $("#btnActualizar").click(function(){ js_reloadgrilla(); });
        $("#selLocalidadUsuarioFiltro, #selMotivo").change(function(){ js_reloadgrilla(); });
        $("#selCompaniaUsuarioFiltro").change(function(){
            $.ajax({
                url: "/localidad/listar/",
                type: "POST",
                data: "selCompaniaUsuario=" + $("#selCompaniaUsuarioFiltro").val(),
                beforeSend:function(){
                    $(".divCargadorPequeno").css("display","inline");
                    $("#selLocalidadUsuarioFiltro").attr("disabled", true);
                },
                success: function(data) {
                    var obj = $("#selLocalidadUsuarioFiltro");
                    obj.html("").append('<option value="">Seleccione</option>');
                    $.each(jQuery.parseJSON(data), function(i,row){
                        obj.append("<option value='"+i+"'>"+row+"</option>");
                    });
                    $(".divCargadorPequeno").css("display","none");
                    obj.attr("disabled", false);
                }
            }); 
        });
        $("#jqtblContenido").jqGrid({
            url: "/evaluacion/paginado/?idCompania="+$("#selCompaniaUsuarioFiltro").val()+
                "&idLocalidad="+$("#selLocalidadUsuarioFiltro").val()+
                "&motivo="+$("#selMotivo").val(),
            datatype: "json",
            height: 460,
            colNames:['idCita','idEmpleado','hdnmotivo','Apellido Paterno','Apellido Materno','Nombres','DNI', 'Motivo de examen', 'hdnExamenes'],
            colModel:[
                {name:'idCita',         index:'idCita',         hidden: true },
                {name:'idEmpleado',     index:'idEmpleado',     hidden: true },
                {name:'hdnmotivo',      index:'hdnmotivo',      hidden: true },
                {name:'appaterno',      index:'appaterno',      width:270, sortable:false, align:'center' },
                {name:'apmaterno',      index:'apmaterno',      width:270, sortable:false, align:'center' },
                {name:'nombres',        index:'nombres',        width:273, sortable:false, align:'center' },
                {name:'dni',            index:'dni',            width:100, sortable:false, align:'center' },
                {name:'motivo',         index:'motivo',         width:350, sortable:false, align:'center' },
                {name:'hdnExamenes',    index:'hdnExamenes',    hidden: true }
            ],
            gridComplete: function() {
                var registros = $("#jqtblContenido").getRowData();
                var c = 0;
                var id = "";
                while(c < registros.length){
                    id = registros[c].idCita;
                    $("#"+id).contextMenu("MenuJqGrid",eventsMenu);
                    c++;
                }
            },
            rownumbers: true,
            rowNum:200,
            pager: "#jqdivPaginado",
            viewrecords: true
        });
        var idCita, idEmpleado, idMotivo;
        var msjCargando = "<div id='divCargadorPrincipal'><img src='/images/loading.gif' /><br>Cargando...</div>";
        function js_reloadgrilla(){
            $("#jqtblContenido").setGridParam({
                "url": "/evaluacion/paginado/?idCompania=" + $("#selCompaniaUsuarioFiltro").val()+
                    "&idLocalidad=" + $("#selLocalidadUsuarioFiltro").val()+
                    "&motivo=" + $("#selMotivo").val()
            }).trigger("reloadGrid");
        }
        $("#dialogAvisoNoPase").dialog({
            autoOpen: false,
            height: 120,
            width: 250,
            modal: true,
            resizable: false,
            open: function(){ },
            close: function() { }
        });
    });
</script>
