        function js_validarDFA7C(){
            var objs, divMensaje, obj = "#frmFichaAnexo7C";
            $("body").css("overflow","hidden");$(".ui-widget-overlay").css("width","100%");
            $("#txtPuestoPostula, #txtPuestoActual").autocomplete({
                source: function( request, response ) {
                    $.ajax({
                        url: "/evaluacion/puestoautocompletar/",
                        dataType: "json",
                        data: { term: request.term },
                        success: function(data) { response($.map(data, function(item) { return { label: $.trim(item.Puesto) }; })); }
                    });
                },
                minLength: 3
            });
            function split( val ) { return val.split( /\n\s*/ ); }
            function extractLast( term ) { return split( term ).pop(); }
            $("#txtObservacioneseval").bind( "keydown", function( event ) {
                if ( event.keyCode === $.ui.keyCode.TAB && $( this ).data( "autocomplete" ).menu.active ) { event.preventDefault(); }
            }).autocomplete({
                source: function( request, response ) {
                    $.ajax({
                        url: "/evaluacion/enfermedadesautocompletar/",
                        dataType: "json",
                        data: { term: extractLast( request.term ) },
                        success: function(data) { response($.map(data, function(item) { return { label: $.trim(item.nombre)+" ("+item.idObservaciones+")" }; })); }
                    });
                },
                focus: function() { return false; },
                select: function( event, ui ) {
                    var terms = split( this.value );
                    terms.pop();
                    terms.push( ui.item.value );
                    terms.push( "" );
                    this.value = terms.join( "\n" );
                    return false;
                },
                minLength: 5
            });
            $(obj).validationEngine("attach", {
                onValidationComplete: function(form, status){
                    if(status){
                        divMensaje = $(obj + " .divDialogTitulo");
                        divMensaje.html("Ingrese la información requerida: ");
                        $.ajax({
                            url: '/evaluacion/fichasietecgrabar/',
                            type: "POST",
                            data: $(obj).serialize(),
                            beforeSend:function(){
                                $(obj).validationEngine("detach");
                                divMensaje.html("<img src='/images/loading.gif' />");
                            },
                            success: function(data) {
                                objs = $.parseJSON(data);
                                if(objs.status == 1){
                                    $("#dialogFichaAnexo7C").dialog("close");
                                    js_reloadgrilla();
                                    if(objs.fin == 5) js_imprimeficha(idCita);
                                }else{
                                    js_validarDFA7C();
                                    divMensaje.attr("class", "txtCen error-text");
                                    divMensaje.html(objs.message);
                                }
                            }
                        });
                    }
                },
                relative: true,
                overflownDIV:"#dialogFichaAnexo7C",
                autoPositionUpdate: true,
                promptPosition : "topLeft",
                scroll: true,
                autoHidePrompt: true,
                autoHideDelay: 3000
            });
        }
